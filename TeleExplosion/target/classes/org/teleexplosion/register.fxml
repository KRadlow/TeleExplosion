<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="488.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.teleexplosion.controlers.RegisterController">
   <top>
      <Text id="registerTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Tele Explosion" BorderPane.alignment="CENTER">
      <BorderPane.margin>
         <Insets top="20.0" />
      </BorderPane.margin></Text>
   </top>
   <center>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="472.0" prefWidth="330.0" spacing="15.0" BorderPane.alignment="CENTER">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Register" />
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose your login:" />
         <TextField alignment="TOP_LEFT" prefHeight="25.0" />
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose your password:" />
         <PasswordField />
         <Button fx:id="registerButton" mnemonicParsing="false" text="Register" />
         <Button mnemonicParsing="false" onAction="#backToLogin" text="Back" />
      </VBox>
   </center>
</BorderPane>
