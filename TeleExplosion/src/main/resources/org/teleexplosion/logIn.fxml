<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="610.0" prefWidth="636.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.teleexplosion.controlers.LogInController">
   <top>
      <Text id="loginTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Tele Explosion" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="20.0" />
         </BorderPane.margin></Text>
   </top>
   <center>
      <VBox fx:id="VBoxLogin" alignment="CENTER" fillWidth="false" prefHeight="569.0" prefWidth="436.0" spacing="15.0" BorderPane.alignment="CENTER">
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Login:" />
         <TextField prefHeight="25.0" scaleShape="false" />
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
         <PasswordField />
         <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="150.0" text="Log In" />
         <Button mnemonicParsing="false" onAction="#register" prefHeight="35.0" prefWidth="150.0" text="Register" />
         <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#backToMenu" prefHeight="35.0" prefWidth="150.0" text="Back" BorderPane.alignment="CENTER">
            <BorderPane.margin>
               <Insets bottom="20.0" />
            </BorderPane.margin></Button>
      </VBox>
   </center>
   <bottom>

   </bottom>
</BorderPane>
